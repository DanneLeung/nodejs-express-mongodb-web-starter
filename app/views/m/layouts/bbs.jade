-var v_js = "0.0.1""
extends default
block header
  style.
    .item {
      background-color: #fff;
    }
    .subtitle{
      margin-bottom: 0.5rem;
    }
    .nickname {margin-right: 0.5rem}
    .list{background-color:#fff;}
    .topic {padding:0 .5rem}
    .topic, .item {cursor:pointer;}
    .imgs a {width: 100%;}
    .imgs .img {height:5rem; max-height:5rem; width: auto; background-size: cover; background-repeat: no-repeat;background-position: center;padding:0.4rem;}
    .comments .item>.content>.imgs {
      margin-top: 0
    }
    .comments .item>.content>.imgs img {
      margin: 0
    }
    .comments>.list {
      box-shadow: inset .1rem 0 0 rgba(0, 0, 0, .1)
    }
    #fabNav {
      display: none!important
    }
block main
  block headNav
    
  //- #pageLoading.loading.loading-light
  #partial.container.no-padding
    block content

block scripts
  script.
    $(document).ready(function(){
      W.config(appid);
    });
  block contentScripts

